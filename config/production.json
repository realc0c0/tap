{
    "app": {
        "name": "Mr. Gnome Tap-to-Earn"
    },
    "server": {
        "port": 80,
        "host": "0.0.0.0",
        "cors": {
            "origin": ["https://your-production-domain.com"],
            "methods": ["GET", "POST"]
        }
    },
    "database": {
        "url": "YOUR_PRODUCTION_SUPABASE_URL",
